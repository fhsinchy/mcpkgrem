#!/Users/farhan/repos/python/mcpkgrm/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mcpkgrm==1.0.0','console_scripts','mcpkgrm'
__requires__ = 'mcpkgrm==1.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mcpkgrm==1.0.0', 'console_scripts', 'mcpkgrm')()
    )
